{
    "name": "frontend-js",
    "version": "0.0.15",
    "license": "MIT",
    "description": "A JavaScript REST client for Frontend.co.",
    "homepage": "https://github.com/frontend-inc/frontend-js",
    "repository": {
        "type": "git",
        "url": "https://github.com/frontend-inc/frontend-js"
    },
    "keywords": [
        "react",
        "nocode",
        "frontend",
        "rest",
        "api"
    ],
    "main": "dist/index.js",
    "exports": {
        ".": "./dist/index.js",
        "./client": "./dist/client/index.js",
        "./components": "./dist/components/index.js",
        "./context": "./dist/context/index.js",
        "./hooks": "./dist/hooks/index.js",
        "./types": "./dist/types/index.js"
    },
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "/dist"
    ],
    "babel": {
        "presets": [
            "react",
            "env",
            "stage-2"
        ]
    },
    "scripts": {
        "build": "npx rimraf dist && npm run build:cjs",
        "build:cjs": "tsc --module commonjs --outDir dist",
        "bump": "./bump-version.sh",
        "deploy": "git add . && git commit -m 'deploy' && git push",
        "push": "yarn bump && yarn build && yarn deploy"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "dependencies": {
        "cookies-next": "^2.1.1"
    },
    "devDependencies": {
        "axios": "^0.21.4",
        "babel-cli": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-2": "^6.24.1",
        "cookies-next": "^2.1.1",
        "react": "^17.0.1 || ^18.0.0",
        "react-dom": "^17.0.2 || ^18.0.0",
        "react-scripts": "^4.0.3",
        "typescript": "^4.4.4"
    },
    "peerDependencies": {
        "axios": "^0.21.4",
        "react": "^17.0.1 || ^18.0.0",
        "react-dom": "^17.0.2 || ^18.0.0"
    }
}
